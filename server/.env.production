# Server configuration
PORT=3001
NODE_ENV=production

# MongoDB connection - Railway automatically provides these environment variables
MONGO_URL=mongodb://${MONGOUSER}:${MONGOPASSWORD}@${MONGOHOST}:${MONGOPORT}/${MONGODATABASE}

# JWT configuration
JWT_SECRET=${JWT_SECRET}

# Client URL for QR codes (no trailing semicolons)
CLIENT_URL=https://soola-production.up.railway.app

# Admin seeding
SEED_ADMIN=false

# CORS configuration
ALLOWED_ORIGINS=https://soola-production.up.railway.app
